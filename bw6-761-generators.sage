#!/usr/bin/env sage

import sys
from sage.all import *

 # prime field and BW6-761 curve
p = 6891450384315732539396789682275657542479668912536150109513790160209623422243491736087683183289411687640864567753786613451161759120554247759349511699125301598951605099378508850372543631423596795951899700429969112842764913119068299
fp=GF(p)
E = EllipticCurve([fp(0),fp(-1)])
E_twist = EllipticCurve([fp(0),fp(4)])

# subgroup order r, cofactor c
r = 258664426012969094010652733694893533536393512754914660539884262666720468348340822774968888139573360124440321458177
c = 26642435879335816683987677701488073867751118270052650655942102502312977592501693353047140953112195348280268661194876
c_twist = 26642435879335816683987677701488073867751118270052650655942102502312977592501693353047140953112195348280268661194869
# assert E.cardinality() == r * c
# assert E_twist.cardinality() == r * c_twist

G = E.random_element()
assert r*G != E(0)
G1 = c*G
assert r*G1 == E(0)
print "E generator:", G1

G_twist = E_twist.random_element()
assert r*G_twist != E_twist(0)
G2 = c_twist * G_twist
assert r*G2 == E_twist(0)
print "E_twist generator:", G2
